#!/usr/bin/make -f
%:
	dh $@

override_dh_auto_clean:
override_dh_auto_configure:
override_dh_auto_build:
override_dh_auto_test:
override_dh_auto_install:

override_dh_install:
	mkdir -p debian/clickjacker/usr/share/clickjacker
	mkdir -p debian/clickjacker/usr/bin
	find . -maxdepth 1 ! -name 'debian' ! -name '.' -exec cp -r {} debian/clickjacker/usr/share/clickjacker/ \;
